# Session secret used by nuxt-auth-utils (required)
NUXT_SESSION_PASSWORD=

# Admin password used by POST /api/auth
# Defaults to "admin" if not set
NUXT_ADMIN_PASSWORD=

# Gate password for /gate
# Defaults to "gate123" if not set (set it in production)
NUXT_GATE_PASSWORD=

# Tencent Cloud COS (required for signing & uploading)
TENCENT_SECRET_ID=
TENCENT_SECRET_KEY=
COS_BUCKET=
COS_REGION=

# Upstash Redis (required for one-time access links + images KV)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Redis key name that stores image COS keys array, e.g. ["folder/a.webp", "folder/b.webp"]
IMAGES_KEY=
