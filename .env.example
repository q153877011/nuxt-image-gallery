# Session secret used by nuxt-auth-utils (required)
NUXT_SESSION_PASSWORD=

# Admin password used by POST /api/auth
# Defaults to "admin" if not set
NUXT_ADMIN_PASSWORD=

# Tencent Cloud COS (required for signing & uploading)
TENCENT_SECRET_ID=
TENCENT_SECRET_KEY=
COS_BUCKET=
COS_REGION=

# Supabase (server-side)
# Used by the backend API to query images by user_id and validate gate tokens.
SUPABASE_URL=
# Recommended (bypasses RLS on server):
SUPABASE_SERVICE_ROLE_KEY=
# Optional fallback (only works if your RLS/policies allow anonymous read):
SUPABASE_ANON_KEY=
